10 R = 70
20 HGR
30 HCOLOR = 3
40 LET SX = INT (256/2)-0.5
50 LET SY = INT (192/2)-0.5
55 HPLOT SX, SY + R
60 FOR X = 0 TO R
70 LET Y = SQRT (R*R-X*X)
80 HPLOT TO SX + X,SY + Y
90 NEXT X
155 HPLOT SX, SY - R
160 FOR X = 0 TO R
170 LET Y = SQRT (R*R-X*X)
180 HPLOT TO SX + X,SY - Y
190 NEXT X
255 HPLOT SX, SY + R
260 FOR X = 0 TO R
270 LET Y = SQRT (R*R-X*X)
280 HPLOT TO SX - X,SY + Y
290 NEXT X
355 HPLOT SX, SY - R
360 FOR X = 0 TO R
370 LET Y = SQRT (R*R-X*X)
380 HPLOT TO SX - X,SY - Y
390 NEXT X
400 END
